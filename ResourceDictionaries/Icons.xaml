<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CarLeasingViewer"
                    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    
    <Style x:Key="Maintance" TargetType="Image">
        <Setter Property="Source" Value="{x:Static local:IconsInfo.MaintenanceIconPath}"/>
    </Style>
    <Style x:Key="Insurance" TargetType="Image">
        <Setter Property="Source" Value="{x:Static local:IconsInfo.InsuranceIconPath}"/>
        <Setter Property="Height" Value="18"/>
    </Style>
</ResourceDictionary>